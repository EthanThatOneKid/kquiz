{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "esnext",
    "esModuleInterop": true,
    "paths": {
      "#src/*": [
        "./src/*"
      ],
      "#lib/*": [
        "./src/lib/*"
      ]
    }
  }
}
